{
  "name": "@adobe/cloud-service-client",
  "version": "2.0.0",
  "description": "Client for working with an HTTP-based cloud service",
  "main": "index.js",
  "browser": "./dist/main.js",
  "license": "Apache-2.0",
  "repository": "adobe/cloud-service-client",
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "testOnly": "./node_modules/.bin/mocha --recursive ./test",
    "build": "./node_modules/.bin/rimraf dist && npm run webpack",
    "prepublishOnly": "npm test && npm run build",
    "coverage": "./node_modules/.bin/nyc npm run testOnly",
    "lint": "./node_modules/.bin/eslint ./src ./e2e ./test",
    "e2e": "./node_modules/.bin/mocha --recursive ./e2e",
    "semantic-release": "semantic-release",
    "webpack": "webpack --config webpack.config.js"
  },
  "author": "Adobe",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Mark Frisbey"
  ],
  "bugs": "https://github.com/adobe/cloud-service-client",
  "dependencies": {
    "node-abort-controller": "^3.0.1",
    "tough-cookie": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@semantic-release/git": "^10.0.1",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "browserslist": "^4.21.2",
    "conventional-changelog-eslint": "^3.0.9",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.26.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^48.2.6",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "minipass-fetch": "^3.0.5",
    "mocha": "^10.0.0",
    "nock": "^13.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "0.13.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^23.1.1",
    "sinon": "^18.0.0",
    "util": "^0.12.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "retry",
    "axios",
    "fetch",
    "eventual",
    "consistency",
    "timeout",
    "poll",
    "http",
    "cloud",
    "cookie",
    "cookies"
  ],
  "engines": {
    "npm": ">=9.6.3 <10.8.0",
    "node": ">=18.20.3 <23.0.0"
  }
}
